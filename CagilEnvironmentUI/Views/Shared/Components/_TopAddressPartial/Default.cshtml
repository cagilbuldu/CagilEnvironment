@using EntityLayer.Concrete
@model List<Address>

<div class="w3l_header_left">	
	@foreach (var item in Model)
	{
		<ul>
			<li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span> @item.Desc4</li>
			<li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>@item.Desc1</li>
			<li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span> @item.Desc2</li>
			<li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> @item.Desc3</li>
			
		</ul>
	}
	
</div>